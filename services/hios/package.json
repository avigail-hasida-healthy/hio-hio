{
  "name": "hios",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "spec": "swagger-jsdoc -d openapiDefinition.json ./src/routes/*.ts ../../tools/api-spec/*.spec -o openapi.json",
    "build-client": "openapi-generator-cli generate -i openapi.json -g typescript-axios -o ../../clients/hios-client -t ../../tools/client-template -p npmName=@gw-workshop/hios-client",
    "build": "npm run spec && npm run build-client && tsc --build",
    "prepare": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
